<% include partials/header-backoffice %>

<div class="container" style="margin-top: 30px;">
  <h1>Utenti registrati JPP</h1>
    <table class="table table-striped table-bordered" style="margin-top: 30px;">
    <thead class="thead-inverse">
      <tr>
        <th>#</th>
        <th>Attivazione</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Modifica</th>
      </tr>
    </thead>
    <tbody>

    <% users.forEach(function(user){ %>
      <tr>
        <td></td>
        <td><input type="checkbox" value=""></td>
        <td><%= user.name %></td>
        <td><%= user.lastname %></td>
        <td><%= user.email %></td>
        <td><button type="button" class="btn btn-primary btn-sm"><a href="/backoffice/users/<%= user._id %>" style="color:#ffffff;">Vedi Dettaglio Utente</a></button></td>
      </tr>
  <% }) %>      
      
    </tbody>
  </table>
</div><!-- /.container -->



<% include partials/footer %>
